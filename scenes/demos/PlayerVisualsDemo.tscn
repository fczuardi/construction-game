resource_name = "runbtn"
script/source = "extends Button

@onready var player_runner: PlayerRunner = %PlayerRunner
@onready var player_visuals_2: PlayerVisuals = %PlayerVisuals2

func _ready() -> void:
    toggled.connect(_on_toggle)
    
func _on_toggle(toggled_on):
    player_visuals_2.set_run_enabled(toggled_on)
    if toggled_on:
        player_runner.set_speed_mode(\"run\")
    else:
        player_runner.set_speed_mode(\"walk\")
        
"

[sub_resource type="GDScript" id="GDScript_eqxwo"]
resource_name = "reset_button"
script/source = "extends Button

@onready var player_runner: PlayerRunner = %PlayerRunner
@onready var player_visuals_2: PlayerVisuals = %PlayerVisuals2
@onready var preset_speed_run: Button = %PresetSpeedRun

func _ready() -> void:
    pressed.connect(_on_pressed)
    
func _on_pressed():
    player_runner.reset_to_start()
    player_visuals_2.set_run_enabled(false)
    preset_speed_run.set_pressed_no_signal(false)
"

